{
    "name": "autowp/autowp.ru",
    "description": "AutoWP.ru, the website",
    "license": "MIT",
    "require": {
    	"php": "^5.6.0",
        "zendframework/zendframework1": "1.*",
        "twbs/bootstrap": "3.*",
        "phpmyadmin/phpmyadmin": "4.*",
        "fortawesome/font-awesome": "^4.5",
        "components/jquery": "^2.1",
        "irazasyed/telegram-bot-sdk": "dev-master",
        "components/jquery-cookie": "1.4.1.2",
        "zendframework/zend-mail": "^2.5",
        "phayes/geophp": "^1.2",
        "ext-curl": "*",
        "ext-imagick": "*",
        "ext-mysqli": "*",
        "ext-memcache": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-intl": "*",
        "ext-gd": "*",
        "michelf/php-markdown": "^1.6",
        "league/html-to-markdown": "^4.1",
        "autowp/textstorage": "^1.0",
        "autowp/image": "^1.0",
        "autowp/filters": "^1.1",
        "autowp/external-login-service": "^1.0",
        "zendframework/zend-uri": "^2.5",
        "zendframework/zend-db": "^2.7",
        "twitter/typeahead.js": "^0.11.1",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-mvc-i18n": "^1.0",
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-paginator": "^2.7",
        "zendframework/zend-view": "^2.7",
        "zendframework/zend-mvc-console": "^1.1",
        "zendframework/zend-form": "^2.9",
        "zendframework/zend-captcha": "^2.5"
    },
    "authors": [
        {
            "name": "Dmitry Pereslegin",
            "email": "dmitry@pereslegin.ru"
        }
    ],
    "autoload": {
        "psr-0": {
            "Project_": "library/"
        },
        "psr-4": {
            "Autowp\\": "library/Autowp",
            "Application\\Form\\": "application/forms/",
            "Application\\Model\\": "application/models",
            "Application\\Model\\DbTable\\": "application/models/DbTable",
            "Application\\Service\\": "application/services"
        },
        "classmap": [
            "application/models/",
            "application/forms/",
            "application/services/",
            "library/Project/Form/Element/",
            "library/Project/Telegram"
        ]
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "AutowpTest\\": "test/"
        }
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/irazasyed/telegram-bot-sdk"
        }
    ],
    "suggest": {
        "ext-geos": "Allows map module"
    }
}
